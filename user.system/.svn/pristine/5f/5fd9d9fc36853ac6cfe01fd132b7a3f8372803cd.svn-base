package xinrui.cloud.domain.dto;

import io.swagger.annotations.ApiModel;
import io.swagger.annotations.ApiModelProperty;
import lombok.Data;
import xinrui.cloud.common.utils.BeanUtilsEchance;
import xinrui.cloud.domain.TechnologyFinancialAppointment;

import java.util.List;


@ApiModel("科技金融预约用户数据传输对象")
@Data
public class TechnologyFinancialAppointmentUserDto extends TechnologyFinancialBaseUserDto {
    @ApiModelProperty(value = "预约时间", position = 8)
    private String appointmentDate;
    @ApiModelProperty(value = "处理时间", position = 9)
    private String processDate;


    public static List<TechnologyFinancialAppointmentUserDto> copy(List<TechnologyFinancialAppointment> technologyFinancialAppointments) {
        return BeanUtilsEchance.copyList(technologyFinancialAppointments, TechnologyFinancialAppointmentUserDto.class, new BeanUtilsEchance.CopyListFilter<TechnologyFinancialAppointmentUserDto>() {
            @Override
            public TechnologyFinancialAppointmentUserDto copy(Object source, TechnologyFinancialAppointmentUserDto target) {
                BeanUtilsEchance.copyPropertiesEnhance(source,target);
                BeanUtilsEchance.copyPropertiesEnhance(((TechnologyFinancialAppointment)source).getTechnologyFinancialUser(),target);
                return target;
            }
        });
    }
}
