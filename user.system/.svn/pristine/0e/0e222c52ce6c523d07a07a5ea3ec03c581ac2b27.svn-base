#设置当前项目内置服务器的端口
server.port=8280
#设置服务器能够接收的消息头的容量
server.max-http-header-size=102400
#erureka configruation
# 声明是否将自己的信息注册到Eureka服务器
eureka.client.register-with-eureka=true
# 是否到Eureka服务器中抓取注册信息
eureka.client.fetch-registry=true   
eureka.client.prefer-ip-address=true
eureka.client.service-url.defaultZone=http://root:root@mag2:8250/eureka
eureka.client.instancelnfoReplicationlntervalSeconds=10
#修改eureka的心跳周期单位为秒
eureka.instance.leaseExpirationDurationlnSecond=90
#设置清理注册表的周期时间，单位为毫秒
eureka.server.eviction-interval-timer-in-ms=6000
#标识服务是否开启自我保护模式
eureka.server.enable-self-preservation=false
#配置注册表抓取间隔时间 （默认30秒），取服务端抓取注册表，并且将服务端的注册表保存到本地缓存
eureka.client.registryFetchIntervalSeconds=30
#配置元数据可在服务之间共享
eureka.instance.metadata-map=元数据
#actuator configruation
endpoints.enabled=true
endpoints.info.sensitive=false
endpoints.health.sensitive=false
management.address=127.0.0.1
management.context-path=/
management.port=8290
