package xinrui.cloud.domain.dto;


import io.swagger.annotations.ApiModel;
import io.swagger.annotations.ApiModelProperty;
import lombok.Getter;
import lombok.Setter;

import java.util.List;

@ApiModel("科技金融产品审核列表对象")
@Getter
@Setter
public class TechnologyFinalAuditDto {

    @ApiModelProperty(value = "被拒绝的产品列表", position = 1)
    private List<TechnologyFinancialDto> refusedList;
    @ApiModelProperty(value = "审核中的产品列表", position = 2)
    private List<TechnologyFinancialDto> applayingList;

    public TechnologyFinalAuditDto() {}

    public TechnologyFinalAuditDto(List<TechnologyFinancialDto> refusedList, List<TechnologyFinancialDto> applayingList) {
        this.refusedList = refusedList;
        this.applayingList = applayingList;
    }
}
