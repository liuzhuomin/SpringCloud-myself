package liuzhuomin.cloud.service.impl;

import liuzhuomin.cloud.domain.Organization;
import liuzhuomin.cloud.domain.OtherGroup;
import liuzhuomin.cloud.dto.OtherGroupDto;
import org.hibernate.criterion.DetachedCriteria;
import org.hibernate.criterion.Restrictions;
import org.springframework.stereotype.Service;

@Service
public class OrganizationServiceImpl extends BaseServiceImpl<Organization> implements liuzhuomin.cloud.service.OrganizationService {
    @Override
    public OtherGroupDto findByCompanyName(String name) {
        DetachedCriteria.forClass(OtherGroup.class).add(Restrictions.eq("name",name));
        return null;
    }
}
